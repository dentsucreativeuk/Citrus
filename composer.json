{
    "name": "whitespace/citrus",
    "description": "Automatically purge and ban cached elements in Varnish",
    "type": "craft-plugin",
    "version": "0.0.61",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "citrus"
    ],
    "support": {
        "docs": "https://github.com/dentsucreativeuk/Citrus",
        "issues": "https://github.com/dentsucreativeuk/Citrus/pulls"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Whitespace",
            "homepage": "https://whitespacers.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "whitespace/varnish-connect": "^0.0.1"
    },
    "autoload": {
        "psr-4": {
          "whitespace\\citrus\\": "src/"
        }
    },
    "extra": {
        "name": "Citrus",
        "handle": "citrus",
        "hasCpSettings": false,
        "hasCpSection": true,
        "changelogUrl": "https://github.com/dentsucreativeuk/Citrus/blob/master/CHANGELOG.md",
        "class": "whitespace\\citrus\\Citrus"
    }
}
